<script>
     $(document).ready(function() {
              $('#textareaBlog').keyup(function() {
                 if ($(this).val()!==''){
                  $('#puplishBlogButton').prop('disabled',false)
                  }
                 else $('#textareaBlog').keydown(function() {
                 $('#puplishBlogButton').prop('disabled',true);
                });
              });
